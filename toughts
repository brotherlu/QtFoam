struct {
	const char* filename;
	std::vector<glm::vec3> Verts;
	std::vector<glm::vec3> Norms;
} asset;

class actor: public QtListItem{
asset* thisAsset;
transform* transforms;
}

struct {
	int type;
	float value;
} transform;
